<?php

/**
 * Template preprocessor for 'meedan_oembed_thumbnail_generic' with provider 'meedan_oembed_bridge'.
 */
function template_preprocess_meedan_oembed_thumbnail_bridge(&$vars) {
  $render = oembedcore_render_cache('oembed', $vars['url'], $vars['attributes']);
  $vars['thumbnail'] = render($render);
}
